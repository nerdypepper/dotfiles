URxvt.tabbed.no-tabbedex-keys: true


! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true

rofi.color-window:    base00,        base00,        base00,             base00,          base05
rofi.color-normal:    base00,        #c9c9c9,        base00,             base00,          base0B
rofi.color-active:    base00,        base0B,        base00,             base00,          base05
rofi.color-urgent:    base00,        base03,        base00,             base00,          base05

rofi.fake-transparency: true
rofi.font: Source Sans Pro 10
rofi.columns: 1
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.location: 0
rofi.padding: 12
rofi.fullscreen: false
rofi.width: 350
rofi.lines: 6

