URxvt.cursorBlink: 1


URxvt.url-launcher: usr/bin/chromium
URxvt.font: xft:Iosevka Nerd Font:size=10
!URxvt.boldFont: xft:flottflott:size=16
URxvt.scrollBar: false
URxvt.internalBorder: 24
URxvt.cursorUnderline : true
URxvt.geometry : 50x25
URxvt.thickness : 5
URxvt.titleBar: true

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true

rofi.color-window:    #f7f3ee,        #f7f3ee,       #f7f3ee,             #f7f3ee,          base05
rofi.color-normal:    #f7f3ee,        #625b50,       #f7f3ee,             #d6e5f2,          #485561
rofi.color-active:    #f7f3ee,        base0B,        #f7f3ee,             #f7f3ee,
rofi.color-urgent:    #f7f3ee,        base03,        #f7f3ee,             #f7f3ee,

rofi.fake-transparency: true
rofi.font: Iosevka Nerd Font 9
rofi.columns: 1
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.location: 0
rofi.padding: 12
rofi.fullscreen: false
rofi.width: 350
rofi.lines: 6

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.autohint: false
Xft.lcdfilter: lcddefault
